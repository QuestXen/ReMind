{
	"files.associations": {
		"*.css": "tailwindcss"
	},
	"editor.quickSuggestions": {
		"strings": "on"
	},

	"rust-analyzer.cargo.buildScripts.overrideCommand": ["cargo", "check", "--message-format=json"],
	"rust-analyzer.cargo.targetDir": true,
	"rust-analyzer.cargo.allTargets": true,
	"rust-analyzer.cargo.sysroot": "discover",
	"rust-analyzer.check.command": "clippy",
	"rust-analyzer.check.extraArgs": ["--", "-Wclippy::pedantic"]
}
